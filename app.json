{
  "expo": {
    "name": "Never Forget",
    "slug": "never-forget",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/NFAppIcon.png",
    "notification": {
      "icon": "./assets/NFAppIcon.png"
    },
    "splash": {
      "image": "./assets/NFicon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.neverforget.neverforget",
      "versionCode": 1,
      "useNextNotificationsApi": true,
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}